  <folder name="Motivation" id="id-4e45311cd6dc46d695f06f794498b679" type="motivation">
    <element xsi:type="archimate:Stakeholder" name="Presidente da software house (Dilan)" id="id-8854fd7ae60148d8b2b8d2cddfe64fc2"/>
    <element xsi:type="archimate:Driver" name="Não pode mais ocorrer atendimentos de suporte de maneira informal pois, clientes estão cancelando contratos com a nossa empresa por insatisfação com o atendimento " id="id-edb69a4b706c47cfbf83d7e5077a02eb"/>
    <element xsi:type="archimate:Value" name="Aumentara a rentenção de clientes(renovação de contratos). Perdemos em media R$ 120 mil reais de contratos cancelados por insatifação com com o suporte técnico aos nossos produtos." id="id-a8173033b8c242ed886833926a272edd"/>
    <element xsi:type="archimate:Value" name="Um cliente satisfeito amplia negócios e indica novos clientes. Ampliamos 280 mil por ano do nosso faturamento com as ampliações de negocios partindo de indicações dos clientes satisfeitos." id="id-42d4ad466d1a44508c0b1da5a463bce3"/>
    <element xsi:type="archimate:Assessment" name="Assessment" id="id-819eb255662a482ebd1b690e17f7d127"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-1923b2aaa25b40fda6731a51b19707a6"/>
    <element xsi:type="archimate:Assessment" name="Registro e acompanhamento dosatendimentos até o encerramento usando um sistema digital(informatizado)" id="id-75b01c2bdb744304a2dff4831d8be016"/>
    <element xsi:type="archimate:Assessment" name="Ter um meio de acompanhar o grau de satisfação dos clientes com o suporte, de maneira formal" id="id-c40aea7de64c46d4b1f179698559610f"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-508ba47eb12f411498001fab8ef007b5"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-abb4bc2fb2ee470a81cf86c3e1cf08f9"/>
    <element xsi:type="archimate:Goal" name="Informatização dos registros ou da abertura de ocorrencias técnicas do suporte " id="id-3e140ffccbcf4db594e3319621639b77"/>
    <element xsi:type="archimate:Goal" name="Informatizar o processo de diagnósticos e soluções de chamados" id="id-729b6e554538446084f301ba66a52b8f"/>
    <element xsi:type="archimate:Goal" name="Permitir que o cliente, atraves do uso do software, possa dar o seu feedback sobre o atendimento, de forma objetiva" id="id-d453217bda764677913030f7580b4987"/>
    <element xsi:type="archimate:Outcome" name="100% dos chamados precisam estar registrados no sistema novo, sendo que essa solução de documentação digital e chamados precisa ser liberada até 31/12/2023." id="id-91c521ef482548c2a8202912fa8ac51a"/>
    <element xsi:type="archimate:Outcome" name="O cliente deve avaliar o atendimento com notas objetivas. Esse sistema de avaliação deve estar disponivel 3 meses após a implementação do sistema de resgistro e acompanhamento de chamados." id="id-472de41013fd4f6697c691dfe7cc3481"/>
    <element xsi:type="archimate:Principle" name="Não obrigar o cliente a responder a pesquisa de satisfação (a avaliação é opção do cliente)" id="id-b9d4db03a5184b529b041fd0e8437638"/>
    <element xsi:type="archimate:Principle" name="Ao solucionar um chamado, a pesquisa de satisfação tem que ser disparada para o cliente." id="id-ca28ddc07d5e45a69c59ff4e2e44dd9a"/>
    <element xsi:type="archimate:Principle" name="Recepcionar os chamados direcionando imediatamente para uma equipe especialzada no tipo de problema que o cliente enfrenta." id="id-eefd5c5d010c4e0284762ec3f1b08c29"/>
    <element xsi:type="archimate:Principle" name="Prover uma base de conhecimento e soluções ja catalogados para agilizar a solução." id="id-aa1e98d7ddac4e88a645689dd332f46a"/>
  </folder>
